<!-- Neuron -->

<!ELEMENT Neuron		(MetaData,
				 Data?,
				 Role*,
                    		 PlaysRoleIn*)>
<!ATTLIST Neuron
	TYPEURI			 CDATA			#REQUIRED>

<!-- END Neuron -->



<!ENTITY % MetaData PUBLIC "-//CID//DTD MetaData 1.0//EN" "MetaData.dtd">
%MetaData;



<!-- Data -->

<!ELEMENT Data			 (Tag*)>

<!-- END Data -->



<!-- Role -->

<!ELEMENT Role			(Multiplicity)>
<!ATTLIST Role
	NEURONURI		 CDATA			#REQUIRED
	TYPE			 CDATA			#REQUIRED>

<!ELEMENT Multiplicity		 EMPTY>
<!ATTLIST Multiplicity
	LOWEST			 CDATA			"0"
	HIGHEST			 CDATA			"infinity">

<!-- END Role -->



<!-- PlaysRoleIn -->

<!ELEMENT PlaysRoleIn		 EMPTY>
<!ATTLIST PlaysRoleIn
	NEURONURI		 CDATA			#REQUIRED>

<!-- END PlaysRoleIn -->

